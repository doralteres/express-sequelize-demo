{
  "name": "express-sequelize-demo",
  "description": "Demo API & Sequelize project usind express and sequelize.",
  "author": {
    "name": "Dor Alteresku"
  },
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "tsc",
    "commit": "cz",
    "release": "standard-version --releaseCommitMessageFormat 'chore(release): {{currentTag}} :dizzy: [skip ci]'",
    "dev": "ts-node-dev --respawn --transpile-only ./src/bin/www.ts"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.18.1",
    "http-errors": "^1.8.1",
    "morgan": "~1.9.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.12.5",
    "sqlite3": "^5.0.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.10",
    "@types/sequelize": "^4.28.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "lodash": "^4.17.21",
    "prettier": "2.6.2",
    "standard-version": "^9.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
